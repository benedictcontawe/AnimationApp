<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="oval" >
            <solid
                android:color="@color/olive" />
            <stroke
                android:color="@color/lime"
                android:dashWidth="8dp"
                android:dashGap="3dp"
                android:width="3dp"/>
        </shape>
    </item>
    <item
        android:top="10dp"
        android:start="10dp"
        android:end="10dp"
        android:bottom="10dp"
        android:drawable="@drawable/ic_plus" />
</layer-list>